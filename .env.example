# Lakesail Configuration
VITE_LAKESAIL_API_KEY=your_lakesail_api_key_here
VITE_LAKESAIL_ENDPOINT=wss://api.lakesail.com/v1/streams

# Development Settings
VITE_ENABLE_ANALYTICS=true
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false

# API Configuration
VITE_API_BASE_URL=https://api.cryptowire.live/v1
VITE_WS_ENDPOINT=wss://api.cryptowire.live/v1/stream

# Feature Flags
VITE_ENABLE_WHALE_DETECTION=true
VITE_ENABLE_DEFI_TRACKING=true
VITE_ENABLE_SENTIMENT_ANALYSIS=true
VITE_ENABLE_CROSS_CHAIN_ANALYSIS=true

# Performance Settings
VITE_MAX_TRANSACTIONS_BUFFER=1000
VITE_UPDATE_INTERVAL=1000
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=10

# Third-party Integrations
VITE_COINGECKO_API_KEY=your_coingecko_api_key
VITE_DEFIPULSE_API_KEY=your_defipulse_api_key

# Analytics
VITE_GA_TRACKING_ID=GA-XXXXXXXXX
VITE_SENTRY_DSN=your_sentry_dsn
